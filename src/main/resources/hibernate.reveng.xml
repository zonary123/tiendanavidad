<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="tienda" match-name="carrito"/>
  <table-filter match-catalog="tienda" match-name="compras"/>
  <table-filter match-catalog="tienda" match-name="historialusuarios"/>
  <table-filter match-catalog="tienda" match-name="productos"/>
  <table-filter match-catalog="tienda" match-name="usuarios"/>
</hibernate-reverse-engineering>